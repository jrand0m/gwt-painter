<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>SVGPainter.html</welcome-file>
  </welcome-file-list>
    <servlet>
        <servlet-name>jUnitHostImpl</servlet-name>
        <servlet-class>com.google.gwt.junit.server.JUnitHostImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>info.jrand0m.code.SVGPainter IntersectService</servlet-name>
        <servlet-class>info.jrand0m.code.server.services.IntersectServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>jUnitHostImpl</servlet-name>
        <url-pattern>/SVGPainter/junithost/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>info.jrand0m.code.SVGPainter IntersectService</servlet-name>
        <url-pattern>/SVGPainter/IntersectService</url-pattern>
    </servlet-mapping>

</web-app>
